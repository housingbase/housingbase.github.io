    text-shadow: 2px 2px 2px black;
}

.playerinfo {
    text-align: center;
}

.houseinfo {
   cursor: pointer;
    position: relative;
    display: flex;
    flex-direction: column;
    align-items: center;
    background-color: rgb(255, 255, 255);
    border:rgb(204, 204, 204), solid, 1px;
    border-radius: 4px;
    width: 300px;
    height: 100px;
    margin-top: 15px;
    margin-bottom: 20px;
    margin-right: 20px;
    padding: 16px; /* inner spacing like addon-card */

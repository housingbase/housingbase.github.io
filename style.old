}

.notification {
    opacity: 0;
    background-color: #333;
    color: white;
    padding: 15px;
    margin-bottom: 10px;
    border-radius: 5px;
    transform: translateX(100%);
    transition: opacity 0.5s ease, transform 0.5s ease;
}

.notification.fade-in {
    opacity: 1;
    transform: translateX(0);

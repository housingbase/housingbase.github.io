#upload-section {
  background: #fff;
  border-radius: 4px;
  padding: 20px;
  margin-top: 20px;
  border: 1px solid #c2c2c2;
}

#upload-form {
  display: flex;
  flex-direction: column;
  gap: 10px;
}

#upload-form input[type="text"], #upload-form input[type="file"] {
  padding: 8px;
  border: 1px solid #ccc;
  border-radius: 4px;
}

#upload-status {
  font-weight: bold;
}

#drop-area {
  border: 2px dashed #8a00e6;
  border-radius: 8px;
  padding: 40px;
  text-align: center;
  margin-bottom: 12px;
  cursor: pointer;
  transition: background 0.2s ease;
}

#drop-area.hover {
  background-color: rgba(0, 115, 230, 0.1);
}

#drop-area p {
  margin: 6px 0;
  font-size: 0.95rem;
  color: #252525;
}

#progress-container {
  width: 100%;
  background: #eee;
  border-radius: 8px;
  margin-top: 12px;
  height: 16px;
  overflow: hidden;
}

#progress-bar {
  height: 100%;
  width: 0%;
  background-color: #0049e6;
  transition: width 0.2s ease;
  border-radius: 8px;
}
/* Scrollbar Styling for Modal Body */

/* Container below header, absolute/fixed to not affect other elements */
.search-container {
    position: absolute; /* take it out of the normal flow */
    top: 220px; /* distance from top of page/header */
    left: 20px; /* distance from left edge */
    z-index: 1000;
    display: flex;
    justify-content: flex-start; /* align to left */
    width: auto; /* shrink to fit content */
}

/* The inner box for styling */
.search-box {
    width: 200px; /* fixed width like login box */
    display: flex;
    flex-direction: column;
    gap: 10px; /* space between textarea and button */
}

/* Make textarea like auth input */
.search-box textarea {
    width: 100%;
    padding: 10px;
    border-radius: 6px;
    font-size: 1rem;
    border: 1px solid #ccc;
    resize: none;
    height: 40px;
    font-family: sans-serif;
    background-color: #f7f7f7;
    color: #000;
    box-sizing: border-box;
}

/* Placeholder style */
.search-box textarea::placeholder {
    color: #999;
}

/* Focus style */
.search-box textarea:focus {
    outline: none;
    border-color: #2a72e5;
}

/* Button style like login page */
.search-box button {
    width: 100%;
    padding: 10px;
    border-radius: 6px;
    font-size: 1rem;
    border: none;
    background: #2a72e5;
    color: #fff;
    cursor: pointer;
    transition: background 0.2s;
}

.search-box button:hover {
    background: #1f5ac3;
}


.nocursor {
    cursor: default;
}

.bigname {
    font-family: 'Monocraft', sans-serif;
    text-align: center;
    color:rgb(0, 0, 0);
}

.bigname a {
    color:rgb(87, 87, 87);
    text-decoration: none;
}

.preoutput {
    text-align: center;
    color: black;
    text-shadow: 2px 2px 0 white;
    user-select: none;
}

#activeOutput {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(350px, 1fr));
    place-items: center;
    margin: auto;
    max-width: 1500px;
}

#houseOutput {
    font-family: 'Monocraft', sans-serif;
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(350px, 1fr));
    place-items: center;
    margin: auto;
    max-width: 1500px;
}

#playerOutput {
    font-family: 'Monocraft', sans-serif; /* custom font */
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(350px, 1fr));
    place-items: center;
    margin: auto;
    max-width: 1300px;
    background-color: #e4e4e4;
    border-radius: 8px;
    border: 1px solid #b1b1b1;
}

.coloredname {
    text-shadow: 2px 2px 2px black;
    will-change: filter;
    transition: all 0.2s ease-in-out;
}

.coloredname:hover {
    filter: brightness(150%);
}

.individualcoloredname {
    text-shadow: 2px 2px 2px black;
    transition: all 0.2s ease-in-out;
}

.rankedname {
    text-shadow: 2px 2px 2px black;
}

.playertext {
    font-weight: bold;
    color: rgb(93, 150, 236);
    text-shadow: 2px 2px 2px black;
}

.cookietext {
    font-weight: bold;
    color: #e67834;
    text-shadow: 2px 2px 2px black;
}

.nodata {
    text-align: center;
    color:rgb(255, 26, 26);
